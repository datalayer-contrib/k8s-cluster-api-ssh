
          set -e
          set -x
          set -o pipefail
          (
          ARCH=amd64
          ${FUNCTIONS}

          if ! yum_upgrade_node; then
            echo >&2 "FATAL: upgrade failed. Quitting."
            exit 1
          fi

          echo "Done..."
          exit 0
          ) 2>&1 | tee -a /var/log/upgrade.log
